<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 p-t-10">
  <%= link_to values_path do %>
    <div class="offer offer-radius offer-success">
      <div class="shape">
        <div class="shape-text">
        </div>
      </div>
      <div class="offer-content">
        <h3 class="lead color-dark m-b-0">
          <%= values.name %>
        </h3>
      </div>
      <p class="p-l-15 p-b-5 p-r-5 align-right"><%= values.created_at.strftime("%d/%m/%Y") %></p>
    </div>
  <% end %>
</div>
