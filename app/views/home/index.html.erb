<!-- start home -->
<section id="home">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-2 col-md-8">
        <h1 class="wow fadeIn" data-wow-offset="50" data-wow-delay="0.9s">Life to <span>Remind</span></h1>
        <div class="element">
          <div class="sub-element">Life To Remind</div>
          <div class="sub-element">Planeje sua vida</div>
          <div class="sub-element">Alcance objetivos</div>
        </div>
        <!-- a data-scroll href="#about" class="btn btn-default wow fadeInUp" data-wow-offset="50" data-wow-delay="0.6s">COMEÇAR</a -->
